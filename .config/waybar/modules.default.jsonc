      "Fog": "ğŸŒ«",
      "HeavyRain": "ğŸŒ§",
      "HeavyShowers": "ğŸŒ§",
      "HeavySnow": "â„ï¸",
      "HeavySnowShowers": "â„ï¸",
      "LightRain": "ğŸŒ¦",
      "LightShowers": "ğŸŒ¦",
      "LightSleet": "ğŸŒ§",
      "LightSleetShowers": "ğŸŒ§",
      "LightSnow": "ğŸŒ¨",
